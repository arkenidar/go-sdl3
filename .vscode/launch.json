{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "[msw] Launch Program",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/app.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "PATH": "${env:PATH};${workspaceFolder}/exe"
            }
        },
        {
            "name": "[gnu] Launch Program",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/app.go",
            "env": {
                "LD_LIBRARY_PATH": "${workspaceFolder}/lib"
            }
        }
    ]
}